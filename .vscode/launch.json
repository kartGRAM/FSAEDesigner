{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-chrome",
        "request": "launch",
        "name": "react debug",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "preLaunchTask": "react-start",
        "postDebugTask": "terminate-react-start",
        "runtimeArgs": [
            " –disk-cache-size=1 –media-cache-size=1 -disk-cache-dir='ZZ:'"
        ],
        "userDataDir": "",
      }
    ]
}
